#include <iostream>
using namespace std;

int main() {
    int a, b, choice;
    cin >> a >> b;
    cin >> choice;

    if (choice == 1)
        cout << a + b;
    else if (choice == 2)
        cout << a - b;
    else if (choice == 3)
        cout << a * b;
    else if (choice == 4)
        cout << a / b;
    else
        cout << "Invalid choice";

    return 0;
}
